appendevent EVENT_OPENMENUSOUND
    set RETURN S_DOOR_CLANK_HUGE
endevent
var sndtmp
var sndtmp2
appendevent EVENT_SOUND
    ifand player[].gm MODE_MENU
    {
        switch RETURN
            case 0 // select (up down)
                set RETURN S_SWITCH_LEVER02
                break
            case 243 // close
                set RETURN S_IMPACT_REV
                getp .sound_pitch sndtmp2
                setp .sound_pitch 1024
                screensound S_DOOR_CLANK_HUGE
                setp .sound_pitch sndtmp2
                break
            case 2 // enter new menu
            case 107
            case 195
            case 196
            case 197
            case 199
            case 226
            case 227
                set RETURN S_DOOR_CLANK_HUGE
                break
            case 213
                set RETURN -1
                break
        endswitch

    }
    else
    {
        ifand tiledata[sprite.picnum].gameflags 32
        {
            switch RETURN 
                case S_CULTIST_ROAM1
                case S_CULTIST_ROAM2
                case S_CULTIST_ROAM3
                case S_CULTIST_CHAT1
                case S_CULTIST_CHAT2
                case S_CULTIST_CHAT3
                case S_CULTIST_FIRE1
                case S_CULTIST_RECOG1
                case S_CULTIST_RECOG2
                case S_CULTIST_RECOG3
                case S_CULTIST_FEAR1
                case S_CULTIST_FEAR2
                case S_SHOTGUNNER_ROAM1
                case S_SHOTGUNNER_ROAM2
                case S_SHOTGUNNER_ROAM3
                case S_SHOTGUNNER_CHAT1
                case S_SHOTGUNNER_CHAT2
                case S_SHOTGUNNER_CHAT3
                case S_SHOTGUNNER_FIRE1
                case S_SHOTGUNNER_RECOG1
                case S_SHOTGUNNER_RECOG2
                case S_SHOTGUNNER_RECOG3
                case S_SHOTGUNNER_FEAR1
                case S_SHOTGUNNER_FEAR2
                case S_GREATER_ASCEND
                case S_GREATER_FLESHBAG
                case S_GREATER_KILL
                case S_GREATER_MINE
                case S_GREATER_UNCLEAN
                    ife sound_enemy_buffer[RETURN] 0
                        setarray sound_enemy_buffer[RETURN] 255
                    else
                        set RETURN -1
                    break
                default
                    ifg RETURN 511
                    {
                        ifl RETURN 658
                        {
                            add enemy_max_stepsounds 1
                            ifg enemy_max_stepsounds ENEMY_MAXSTEPS
                                set RETURN -1
                        }
                    }
                    break
            endswitch
        }
        switch RETURN
            case 2
            case 14
            case 18
            case 20
            case 22
            case 23
            case 24
            case 25
            case 36
            case 40
            case 42
            case 48
            case 81
            case 200
            case 211
            case 212
            case 213
            case 245
            case 3000
                set RETURN -1
                break
            case 71
            case 73
                ifactor SECTOREFFECTOR
                {
                    ife sprite.lotag 17
                    {
                        set db_temp RETURN
                        set RETURN -1
                    }
                }
                break
            case 158
                ifactor A_MECHSECT
                    nullop
                else
                    set RETURN -1
                break
            case 209
                ife nogrunting 1
                    set RETURN -1
                else ifn p_eol 0
                    set RETURN -1
                else ifactorsound player[].i S_SHELLY_SEARCH1
                    set RETURN -1
                else ifactorsound player[].i S_SHELLY_SEARCH2
                    set RETURN -1
                else ifactorsound player[].i S_SHELLY_SEARCH3
                    set RETURN -1
                else ifactorsound player[].i S_SHELLY_SEARCH4
                    set RETURN -1
                else ifactorsound player[].i S_SHELLY_SEARCH5
                    set RETURN -1
                else ifactorsound player[].i S_SHELLY_SEARCH6
                    set RETURN -1
                else ifactorsound player[].i S_SHELLY_SEARCH7
                    set RETURN -1
                else ifactorsound player[].i S_SHELLY_SEARCH8
                    set RETURN -1
                else ifactorsound player[].i S_SHELLY_SEARCHTALK1
                    set RETURN -1
                else ifactorsound player[].i S_SHELLY_SEARCHTALK2
                    set RETURN -1
                else
                {
                    set sndtmp S_SHELLY_SEARCH8
                    sub sndtmp S_SHELLY_SEARCH1
                    rand RETURN sndtmp
                    add RETURN S_SHELLY_SEARCH1
                }
                break
            case 207
                ife nogrunting 1
                    set RETURN -1
                else ifn p_eol 0
                    set RETURN -1
                else ifactorsound player[].i S_SHELLY_SEARCHTALK1
                    set RETURN -1
                else ifactorsound player[].i S_SHELLY_SEARCHTALK2
                    set RETURN -1
                else
                {
                    stopactorsound player[].i S_SHELLY_SEARCH1
                    stopactorsound player[].i S_SHELLY_SEARCH2
                    stopactorsound player[].i S_SHELLY_SEARCH3
                    stopactorsound player[].i S_SHELLY_SEARCH4
                    stopactorsound player[].i S_SHELLY_SEARCH5
                    stopactorsound player[].i S_SHELLY_SEARCH6
                    stopactorsound player[].i S_SHELLY_SEARCH7
                    stopactorsound player[].i S_SHELLY_SEARCH8
                    rand RETURN 1
                    add RETURN S_SHELLY_SEARCHTALK1
                }
                break
        endswitch
        ife player[].gm 0
            nullop
        else ifg p_eol 0
        {
            ifn sprite[].picnum APLAYER
                set RETURN -1
        }
    }
endevent
