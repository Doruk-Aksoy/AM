var bis_temp
var bis_last
var bis_max
defstate bullet_impact_sound
    ifg bis_max 5
        break
    rand bis_temp 2
    ife bis_last bis_temp
    {
        add bis_temp 1
        mod bis_temp 3
    }
    set bis_last bis_temp
    switch surface_type
        case SURFACE_METAL
            add bis_temp S_BULLET_METAL1
            break
        case SURFACE_GLASS
        case SURFACE_LIGHT
            add bis_temp S_BULLET_GLASS1
            break
        case SURFACE_WOOD
            add bis_temp S_BULLET_WOOD1
            break
        case SURFACE_DIRT
            add bis_temp S_BULLET_DIRT1
            break
        case SURFACE_GRASS
            add bis_temp S_BULLET_GRASS1
            break
        case SURFACE_WATER
        case SURFACE_SLIME
            add bis_temp S_BULLET_WATER1
            break
        case SURFACE_FABRIC
        case SURFACE_CARPET
        case SURFACE_PAPER
            add bis_temp S_BULLET_FABRIC1
            break
        case SURFACE_FLESH
            add bis_temp S_BULLET_FLESH1
            break
        default
            add bis_temp S_BULLET_STONE1
            break
    endswitch
    ifpdistl 9216
        soundvar bis_temp
    add bis_max 1
    switch surface_type
        case SURFACE_LIGHT
        case SURFACE_ELECTRICAL
            setp[].sound_pitch 512
            rand bis_temp 2
            add bis_temp S_ENV_ZAP1
            ifpdistl 9216
                soundvar bis_temp
            setp[].sound_pitch 0
            break
    endswitch
    ifrnd 8
    {
        rand bis_temp 4
        add bis_temp S_BULLET_RICOCHET1
        ifpdistl 9216
            soundvar bis_temp
    }
ends

