            state remove_hitscan
            set bbomb_reflect 1
            break
    endswitch
    
    switch sprite.picnum
        case 6466
        case 6532
        case 8818
        case 8819
        case 10332
        case 10408
        case 10740
        case 10742
            geta .cstat temp
            ifand temp 4
            {
                xor temp 4
                seta .cstat temp
            }
            break
        case 6611
            geta .cstat temp
            ifand temp 4
            {
                xor temp 4
                seta .cstat temp
                ifand temp 16
                {
                    geta .ang temp
                    add temp 1024
                    and temp 2047
                    seta .ang temp
                }
            }
            break
    endswitch
endevent

defstate check_vent
    set vent_spr 0
    switch surface_type
        case T_GRATE
        case T_GRATEBROKE
        case T_FAN
        case T_FANBROKE
            set vent_spr 1
            break
    endswitch
ends

