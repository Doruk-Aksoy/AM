defstate player_footstep
    state get_surface_type
    rand step_temp 3
    mod step_temp 3

    ifn sector[].lotag 2
    {
        switch p_steptype
        {
            case 0
                state steptype0
                break
            case 1
                state steptype1
                break
            case 2
                state steptype2
                break
            case 3
                state steptype3
                break
        }
        endswitch

        ifn p_steptype -1
        {
            findnearsprite A_GENERIC_BLOODSPLATTER 512 step_temp2
            ifn step_temp2 -1
            {
                ife sprite[].sectnum sprite[step_temp2].sectnum
                {
                    ifand sprite[step_temp2].cstat 32
                    {
                        ifg sprite[step_temp2].xrepeat 4
                        {
                            ifactor APLAYER
                            {
                                switch sprite[step_temp2].pal
                                    case 0
                                    case 63
                                        set p_footpal 0
                                        break
                                    default
                                        set p_footpal sprite[step_temp2].pal
                                        break
                                endswitch
                                geta[step_temp2].xrepeat step_temp2
                                shiftr step_temp2 2
                                ifn p_footpal 0
                                    shiftr step_temp2 2
                                setp[].footprintcount step_temp2


                            }
                            rand step_temp2 3
                            mod step_temp2 3
                            switch p_steptype
                            {
                                case 0
                                    add step_temp2 S_STEP_WATER_WALK1
                                    break
                                case 1
                                    add step_temp2 S_STEP_WATER_RUN1
                                    break
                                case 2
                                    add step_temp2 S_STEP_WATER_JUMP1
                                    break
                                case 3
                                    add step_temp2 S_STEP_WATER_LAND1
                                    break
                            }
                            endswitch
                            soundvar step_temp2
                        }
                    }
                }
            }
        }
    }
    else ife sector[].lotag 2
        add step_temp S_STEP_WATER_SWIM1

    soundvar step_temp
    ife THISACTOR player.i
    {
        ifn sector.lotag 1
            ifn sector.lotag 2
            {
                ifg p_pup_jump_time 0
                {
                    ifp ponground
                        sound S_BOOTS_STEP
                }
            }
    }
    setp[].sound_pitch 0
    set p_xvel 0
    set p_zvel 0
    set p_steptype 0
ends

