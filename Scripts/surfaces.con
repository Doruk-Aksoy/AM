// Kill HITSCAN on certain sprites
var bbomb_reflect 0 2
defstate remove_hitscan
    ife sprite[].statnum STAT_FALLER
        break
    geta[].cstat temp
    ifand temp 256
        xor temp 256
    seta[].cstat temp
    changespritestat THISACTOR 0
ends

