    ifl temp 2
    {
        for temp range 30720 // MAXTILES
        {
            state propogate_surfaces
            state propogate_damage_surfaces
        }
        setarray pre_surface_type[30720] 1
        setarray pre_surface_damage[30720] 1
    }
endevent

appendevent EVENT_DAMAGEWALL
    ifn wall[RETURN].picnum 9250 ifg wall[RETURN].picnum 0
    {
        switch sprite[THISACTOR].picnum
            case BULLET
            case EXPLOSION
                getw[RETURN].picnum dwall_temp
                set dwall_temp pre_surface_damage[dwall_temp]
                ifg dwall_temp 0
                    setw[RETURN].picnum dwall_temp
                break
            default
                ifg sprite[THISACTOR].extra 0
                {
                    getw[RETURN].picnum dwall_temp
                    set dwall_temp pre_surface_damage[dwall_temp]
                    ifg dwall_temp 0
                        setw[RETURN].picnum dwall_temp
                }
                break
        endswitch
    }
endevent

